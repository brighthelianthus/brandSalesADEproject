{
	"name": "jsonplaceholder_posts_to_blob_input",
	"properties": {
		"activities": [
			{
				"name": "restapitoinputcsv",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "josnplaceholder_posts",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "blobstorageCSV",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "TriggerBasedProject"
		},
		"annotations": []
	}
}