{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfendtoendproj"
		},
		"er2proj_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'er2proj_ADLS'"
		},
		"ADVWORKS_e2eproj_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adventureworks-server-01.database.windows.net"
		},
		"ADVWORKS_e2eproj_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "onlinedb-advworks"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "mycompany-brands-partnersales.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "brandSales"
		},
		"er2proj_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://e2eproj.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfendtoendproj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"er2proj_ADLS_accountKey": {
						"value": "[parameters('er2proj_ADLS_accountKey')]"
					},
					"ADVWORKS_e2eproj_properties_typeProperties_server": {
						"value": "[parameters('ADVWORKS_e2eproj_properties_typeProperties_server')]"
					},
					"ADVWORKS_e2eproj_properties_typeProperties_database": {
						"value": "[parameters('ADVWORKS_e2eproj_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"er2proj_ADLS_properties_typeProperties_url": {
						"value": "[parameters('er2proj_ADLS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}